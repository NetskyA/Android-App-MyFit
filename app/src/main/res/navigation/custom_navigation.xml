<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/custom_navigation"
    app:startDestination="@id/customsIngredients">
    <fragment
        android:id="@+id/customsIngredients"
        android:name="com.example.myfit.MenuCustom.CustomsIngredients"
        android:label="fragment_customs_ingredients"
        tools:layout="@layout/fragment_customs_ingredients" />
    <action
        android:id="@+id/action_global_customsIngredients"
        app:destination="@id/customsIngredients" />
    <fragment
        android:id="@+id/customsResult"
        android:name="com.example.myfit.MenuCustom.CustomsResult"
        android:label="fragment_customs_result"
        tools:layout="@layout/fragment_customs_result" />
    <action
        android:id="@+id/action_global_customsResult"
        app:destination="@id/customsResult" />
</navigation>