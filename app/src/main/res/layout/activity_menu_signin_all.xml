<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            tools:context=".SignAll.MenuSigninAll"
            style="@style/Backgrounds">

            <VideoView
                android:id="@+id/videoView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.07" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Upper"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="120dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Bottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.95" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.93" />

<!--            <pl.droidsonroids.gif.GifImageView
                android:layout_width="140dp"
                android:layout_height="70dp"
                android:src="@drawable/logo"
                android:layout_marginTop="50dp"
                app:layout_constraintBottom_toTopOf="@+id/Upper"
                app:layout_constraintEnd_toStartOf="@+id/Right"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@id/Left"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.533" />-->

            <TextView
                android:id="@+id/text1"
                android:layout_width="@dimen/_240sdp"
                android:layout_height="wrap_content"
                android:text="Discover what's best for your health!"
                android:textSize="@dimen/_25ssp"
                android:textColor="@color/white"
                android:layout_marginTop="@dimen/_30sdp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/Right"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/Left"
                app:layout_constraintTop_toTopOf="@+id/Tengah" />

            <TextView
                android:id="@+id/text2"
                android:layout_width="@dimen/_200sdp"
                android:layout_height="wrap_content"
                android:text="Let health take you far"
                android:textSize="@dimen/_14ssp"
                android:layout_marginTop="@dimen/_70sdp"
                android:textColor="@color/grey"
                app:layout_constraintEnd_toStartOf="@+id/Right"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/Left"
                app:layout_constraintTop_toTopOf="@+id/text1" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Tengah"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="@dimen/_325sdp" />


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Tengahtengah"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="630dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_260sdp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal"
                app:layout_constraintEnd_toStartOf="@+id/Right"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/Left"
                app:layout_constraintTop_toTopOf="@+id/Tengahtengah">

                <LinearLayout
                    android:id="@+id/linearLayout22"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_260sdp"
                    android:background="@drawable/backgroundnavigations2"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/nextSignIn"
                            android:layout_width="@dimen/_265sdp"
                            android:layout_height="@dimen/_35sdp"
                            android:text="Create new account"
                            android:layout_marginTop="@dimen/_15sdp"
                            android:layout_gravity="center"
                            android:textSize="@dimen/_15ssp"
                            android:textColor="@color/white"
                            android:textAllCaps="false"
                            android:background="@drawable/borderblue"
                            android:elevation="@dimen/_4sdp"
                            app:layout_constraintEnd_toStartOf="@+id/Right"
                            app:layout_constraintStart_toStartOf="@+id/Left"
                            app:layout_constraintTop_toTopOf="@+id/signinwithphone"
                            tools:ignore="HardcodedText" />

                        <LinearLayout
                            android:id="@+id/linearLayouttoLogin"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/cheattoLogin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAlignment="center"
                                android:textColor="#8E8E8E"
                                android:textStyle="bold"
                                android:text="Already have an account? "
                                tools:ignore="HardcodedText" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout"
                            android:layout_width="@dimen/_258sdp"
                            android:layout_height="@dimen/_35sdp"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:layout_gravity="center"
                            android:orientation="horizontal">

                            <View
                                android:layout_width="0dp"
                                android:layout_height="@dimen/_1sdp"
                                android:layout_gravity="center"
                                android:layout_weight="1"
                                android:background="@color/grey" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_10sdp"
                                android:layout_gravity="center"
                                android:text="OR"
                                android:textColor="#8E8E8E"
                                android:textSize="@dimen/_12ssp"
                                tools:ignore="HardcodedText" />

                            <View
                                android:layout_width="0dp"
                                android:layout_height="@dimen/_1sdp"
                                android:layout_gravity="center"
                                android:layout_weight="01"
                                android:background="@color/grey" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_40sdp"
                            android:orientation="horizontal"
                            android:layout_gravity="center">
                            <Button
                                android:id="@+id/signwithgoogle"
                                android:layout_width="@dimen/_36sdp"
                                android:layout_height="@dimen/_36sdp"
                                android:background="@drawable/borders"
                                android:textAllCaps="false"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:textSize="@dimen/_16ssp"
                                android:textColor="@color/grey"
                                android:drawableStart="@drawable/google"
                                android:paddingStart="@dimen/_9sdp"
                                app:layout_constraintEnd_toStartOf="@+id/Right"
                                app:layout_constraintHorizontal_bias="0.0"
                                app:layout_constraintStart_toStartOf="@+id/Left"
                                app:layout_constraintTop_toTopOf="@+id/linearLayout"
                                tools:ignore="HardcodedText" />

                            <Button
                                android:id="@+id/signwithumberphone"
                                android:layout_width="@dimen/_36sdp"
                                android:layout_height="@dimen/_36sdp"
                                android:textSize="@dimen/_16ssp"
                                android:drawableStart="@drawable/telephone"
                                android:textColor="@color/grey"
                                android:paddingStart="@dimen/_9sdp"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:textAllCaps="false"
                                android:background="@drawable/borders"
                                app:layout_constraintEnd_toStartOf="@+id/Right"
                                app:layout_constraintStart_toStartOf="@+id/Left"
                                app:layout_constraintTop_toTopOf="@+id/signwithgoogle"
                                tools:ignore="HardcodedText" />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
</layout>