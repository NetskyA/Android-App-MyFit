<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuFeed.MenuFeedOpened">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/main"
            style="@style/Backgrounds"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.03" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Upper"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="120dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Bottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.95" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.97" />

            <LinearLayout
                android:id="@+id/apaja"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                tools:ignore="MissingConstraints">

                <LinearLayout
                    android:layout_width="@dimen/_275sdp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_45sdp"
                    android:orientation="horizontal">

                    <ImageButton
                        android:id="@+id/ibAccount"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_gravity="center_vertical"
                        android:background="#00FFFFFF"
                        android:scaleType="centerInside"
                        android:src="@drawable/back2" />

                    <TextView
                        android:id="@+id/tvAccount"
                        android:layout_width="@dimen/_195sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:text="Explore"
                        android:textAlignment="center"
                        android:textColor="@color/green_main"
                        android:textSize="@dimen/_20sdp"
                        android:textStyle="bold" />

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/profileMFO"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_gravity="end"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:src="@drawable/profileuser" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:layout_marginBottom="@dimen/_20sdp"
                    android:background="@drawable/backgroundnavigations2"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/gambarkcontent"
                        android:layout_width="@dimen/_290sdp"
                        android:layout_height="@dimen/_210sdp"
                        android:layout_gravity="center"
                        app:cardCornerRadius="@dimen/_15ssp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView5"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/examplemenu"
                            android:scaleType="centerCrop"
                            app:layout_constraintHorizontal_bias="0.5"
                            app:layout_constraintStart_toStartOf="parent"/>
                    </androidx.cardview.widget.CardView>

                    <LinearLayout
                        android:layout_width="@dimen/_280sdp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tvTitleMFO"
                            android:layout_width="@dimen/_245sdp"
                            android:layout_height="wrap_content"
                            android:paddingStart="@dimen/_5sdp"
                            android:text="Juice jembod"
                            android:textColor="@color/green_main"
                            android:textSize="@dimen/_18ssp"
                            android:textStyle="bold" />

                        <ImageView
                            android:id="@+id/imagefeed2"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_gravity="center"
                            android:scaleType="centerCrop"
                            android:src="@drawable/favoritebefore" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_280sdp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tvLikeFMO"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:gravity="end"
                            android:paddingStart="@dimen/_10sdp"
                            android:paddingEnd="@dimen/_2sdp"
                            android:text="0"
                            android:textSize="@dimen/_13ssp" />


                        <TextView
                            android:id="@+id/tvby"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:layout_marginStart="@dimen/_2sdp"
                            android:layout_marginEnd="@dimen/_2sdp"
                            android:text="Create by"
                            android:textSize="@dimen/_13ssp" />

                        <TextView
                            android:id="@+id/tvUsernameMFO"
                            android:layout_width="@dimen/_203sdp"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:textColor="@color/green_main"
                            android:textStyle="bold"
                            android:textSize="@dimen/_13ssp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageButton
                            android:id="@+id/feedOpen_ibHide"
                            android:layout_width="@dimen/_15sdp"
                            android:layout_height="@dimen/_15sdp"
                            android:background="@null"
                            android:scaleType="centerCrop"
                            android:src="@drawable/hide" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/feedOpen_layoutDetail"
                        android:layout_width="@dimen/_280sdp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="@dimen/_260sdp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:gravity="center_vertical"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="start"
                                android:text="Nutrition"
                                android:textSize="@dimen/_13ssp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:layout_width="@dimen/_6sdp"
                                    android:layout_height="@dimen/_6sdp"
                                    android:layout_marginTop="@dimen/_7sdp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/circle" />

                                <TextView
                                    android:id="@+id/MenuFeed_tvNutrition"
                                    android:layout_width="@dimen/_240sdp"
                                    android:layout_height="wrap_content"
                                    android:gravity="start"
                                    android:paddingStart="@dimen/_6sdp"
                                    android:text="In publishing and graphic design, Lorem ipsum is a placeholder text commonly used to demonstrate the visual form of a document or a typeface without relying on meaningful content. Lorem ipsum may be used as a"
                                    android:textSize="@dimen/_13ssp" />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="@dimen/_260sdp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:gravity="center_vertical"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="start"
                                android:text="Ingredients"
                                android:textSize="@dimen/_13ssp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:layout_width="@dimen/_6sdp"
                                    android:layout_height="@dimen/_6sdp"
                                    android:layout_marginTop="@dimen/_7sdp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/circle" />

                                <TextView
                                    android:id="@+id/MenuFeed_tvIngredients"
                                    android:layout_width="@dimen/_240sdp"
                                    android:layout_height="wrap_content"
                                    android:gravity="start"
                                    android:paddingStart="@dimen/_6sdp"
                                    android:text="In publishing and graphic design, Lorem ipsum is a placeholder text commonly used to demonstrate the visual form of a document or a typeface without relying on meaningful content. Lorem ipsum may be used as a"
                                    android:textSize="@dimen/_13ssp" />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="@dimen/_260sdp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:gravity="center_vertical"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="start"
                                android:text="How to Make"
                                android:textSize="@dimen/_13ssp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:layout_width="@dimen/_6sdp"
                                    android:layout_height="@dimen/_6sdp"
                                    android:layout_marginTop="@dimen/_7sdp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/circle" />

                                <TextView
                                    android:id="@+id/MenuFeed_tvHowToMake"
                                    android:layout_width="@dimen/_240sdp"
                                    android:layout_height="wrap_content"
                                    android:gravity="start"
                                    android:paddingStart="@dimen/_6sdp"
                                    android:text="In publishing and graphic design, Lorem ipsum is a placeholder text commonly used to demonstrate the visual form of a document or a typeface without relying on meaningful content. Lorem ipsum may be used as a"
                                    android:textSize="@dimen/_13ssp" />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="@dimen/_260sdp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:gravity="center_vertical"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="start"
                                android:text="Notes"
                                android:textSize="@dimen/_13ssp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:layout_width="@dimen/_6sdp"
                                    android:layout_height="@dimen/_6sdp"
                                    android:layout_marginTop="@dimen/_7sdp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/circle" />

                                <TextView
                                    android:id="@+id/MenuFeed_tvNotes"
                                    android:layout_width="@dimen/_240sdp"
                                    android:layout_height="wrap_content"
                                    android:gravity="start"
                                    android:paddingStart="@dimen/_6sdp"
                                    android:text="In publishing and graphic design, Lorem ipsum is a placeholder text commonly used to demonstrate the visual form of a document or a typeface without relying on meaningful content. Lorem ipsum may be used as a"
                                    android:textSize="@dimen/_13ssp" />

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <TextView
                        android:id="@+id/uploadcontentfeed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginEnd="@dimen/_15sdp"
                        android:layout_marginBottom="@dimen/_15sdp"
                        android:text="Mei 2024"
                        android:textColor="@color/green_main"
                        android:textSize="@dimen/_10ssp"
                        app:layout_constraintEnd_toStartOf="@+id/Right"
                        app:layout_constraintStart_toStartOf="@+id/Left"
                        app:layout_constraintTop_toTopOf="@+id/apaja"
                        tools:ignore="HardcodedText" />

                </LinearLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>