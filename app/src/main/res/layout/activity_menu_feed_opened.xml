<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuFeed.MenuFeedOpened">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/main"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/Backgrounds">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.03" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Upper"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_begin="120dp" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Bottom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.95" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/Right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.97" />

            <LinearLayout
                android:id="@+id/apaja"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                tools:ignore="MissingConstraints">

                <LinearLayout
                    android:layout_width="@dimen/_275sdp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_45sdp"
                    android:orientation="horizontal">

                    <ImageButton
                        android:id="@+id/ibAccount"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:scaleType="centerInside"
                        android:layout_gravity="center_vertical"
                        android:background="#00FFFFFF"
                        android:src="@drawable/back2" />

                    <TextView
                        android:id="@+id/tvAccount"
                        android:layout_width="@dimen/_195sdp"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:textColor="@color/blue_main"
                        android:text="Explore"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:textStyle="bold"
                        android:textSize="@dimen/_20sdp" />

                    <ImageView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_gravity="end"
                        android:src="@drawable/profileuser"
                        android:layout_marginStart="@dimen/_5sdp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:background="@drawable/backgroundnavigations2"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/gambarkcontent"
                        android:layout_width="@dimen/_290sdp"
                        android:layout_height="@dimen/_210sdp"
                        android:layout_gravity="center"
                        android:background="@drawable/backgroundnavigations3"
                        android:orientation="horizontal">

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_280sdp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Aldiaafn"
                            android:textStyle="bold"
                            android:textColor="@color/blue_main"
                            android:gravity="start"
                            android:paddingStart="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:textSize="@dimen/_15ssp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_280sdp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/likefeed2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:layout_gravity="center"
                            android:gravity="end"
                            android:paddingStart="@dimen/_10sdp"
                            android:paddingEnd="@dimen/_7sdp"
                            android:textColor="@color/blue_main"
                            android:textSize="@dimen/_15ssp" />

                        <TextView
                            android:layout_width="@dimen/_220sdp"
                            android:layout_height="wrap_content"
                            android:text="likes"
                            android:layout_gravity="center"
                            android:gravity="start"
                            android:textColor="@color/blue_main"
                            android:textSize="@dimen/_15ssp" />

                        <ImageView
                            android:id="@+id/imagefeed2"
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_gravity="center"
                            android:src="@drawable/favoritebefore"
                            android:paddingTop="@dimen/_3sdp"
                            android:scaleType="centerCrop" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_280sdp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginStart="@dimen/_6sdp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="@dimen/_270sdp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2sdp"
                            android:layout_marginEnd="@dimen/_4sdp"
                            android:layout_marginBottom="@dimen/_4sdp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="@dimen/_10sdp"
                                        android:layout_height="@dimen/_10sdp"
                                        android:src="@drawable/circle"
                                        android:layout_marginTop="@dimen/_4sdp"
                                        android:layout_marginEnd="@dimen/_3sdp"
                                        android:layout_marginBottom="@dimen/_5sdp"/>

                                    <TextView
                                        android:layout_width="@dimen/_100sdp"
                                        android:layout_height="wrap_content"
                                        android:text="NUTRITION"
                                        android:textColor="@color/blue_main"
                                        android:textStyle="bold"
                                        android:textSize="@dimen/_15ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvCarbohydrat"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Carbohydrat :"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvCarbohydratValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvCholesterol"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Cholesterol :"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvCholesterolValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvVitaminC"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Vitamin C :"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvVitaminCValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvVitaminD"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Vitamin D :"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvVitaminDValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvCalcium"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Calcium :"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvCalciumValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvCalories"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Calories:"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvCaloriesValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvSodium"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Sodium :"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvSodiumValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvProtein"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Protein:"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvProteinValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvSugar"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Sugar:"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvSugarValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvFat"
                                        android:layout_width="@dimen/_84sdp"
                                        android:layout_height="wrap_content"
                                        android:text="Fat :"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvFatValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:layout_marginStart="@dimen/_5sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvGlycemicIndex"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Glycemic Index:"
                                        android:textStyle="bold"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvGlycemicIndexValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:textStyle="bold"
                                        android:paddingStart="@dimen/_4sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvGlycemicLoad"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Glycemic Load :"
                                        android:textStyle="bold"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvGlycemicLoadValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:textStyle="bold"
                                        android:paddingStart="@dimen/_4sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvNutritionScore"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Nutrition Score :"
                                        android:textStyle="bold"
                                        android:textSize="@dimen/_14ssp" />

                                    <TextView
                                        android:id="@+id/tvNutritionScoreValue"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="100"
                                        android:textStyle="bold"
                                        android:paddingStart="@dimen/_2sdp"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="@dimen/_10sdp"
                                        android:layout_height="@dimen/_10sdp"
                                        android:src="@drawable/circle"
                                        android:layout_marginTop="@dimen/_4sdp"
                                        android:layout_marginEnd="@dimen/_3sdp"
                                        android:layout_marginBottom="@dimen/_5sdp"/>

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="IGREDIENTS"
                                        android:textStyle="bold"
                                        android:textColor="@color/blue_main"
                                        android:textSize="@dimen/_15ssp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/_14sdp"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tvIngredientname1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="Bell pepper"
                                        android:textSize="@dimen/_14ssp" />

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_2sdp"
                            android:layout_marginEnd="@dimen/_4sdp"
                            android:layout_marginBottom="@dimen/_4sdp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/_10sdp"
                                    android:layout_height="@dimen/_10sdp"
                                    android:src="@drawable/circle"
                                    android:layout_marginTop="@dimen/_4sdp"
                                    android:layout_marginEnd="@dimen/_3sdp"
                                    android:layout_marginBottom="@dimen/_5sdp"/>

                                <TextView
                                    android:layout_width="@dimen/_100sdp"
                                    android:layout_height="wrap_content"
                                    android:text="DESCRIPTION"
                                    android:textStyle="bold"
                                    android:textSize="@dimen/_15ssp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_50sdp"
                                    android:hint="Notes"
                                    android:textStyle="italic"
                                    android:layout_marginStart="@dimen/_10sdp"
                                    android:layout_marginEnd="@dimen/_5sdp"
                                    android:background="@drawable/backgroundnavigation1"
                                    android:padding="@dimen/_5sdp"
                                    android:textSize="@dimen/_14ssp"/>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_270sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginEnd="@dimen/_4sdp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/_10sdp"
                                android:layout_height="@dimen/_10sdp"
                                android:src="@drawable/circle"
                                android:layout_marginTop="@dimen/_4sdp"
                                android:layout_marginEnd="@dimen/_3sdp"
                                android:layout_marginBottom="@dimen/_5sdp"/>

                            <TextView
                                android:layout_width="@dimen/_100sdp"
                                android:layout_height="wrap_content"
                                android:text="DESCRIPTION"
                                android:textStyle="bold"
                                android:textColor="@color/blue_main"
                                android:textSize="@dimen/_15ssp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_50sdp"
                                android:hint="Notes"
                                android:textStyle="italic"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:background="@drawable/backgroundnavigation1"
                                android:padding="@dimen/_5sdp"
                                android:textSize="@dimen/_14ssp"/>

                        </LinearLayout>

                    </LinearLayout>

                    <TextView
                        android:id="@+id/uploadcontentfeed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Mei 2024"
                        android:layout_gravity="end"
                        android:layout_marginEnd="@dimen/_15sdp"
                        android:layout_marginBottom="@dimen/_15sdp"
                        android:textColor="@color/blue_main"
                        android:textSize="@dimen/_10ssp"
                        app:layout_constraintEnd_toStartOf="@+id/Right"
                        app:layout_constraintStart_toStartOf="@+id/Left"
                        app:layout_constraintTop_toTopOf="@+id/apaja"
                        tools:ignore="HardcodedText" />

                </LinearLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>