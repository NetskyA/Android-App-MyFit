<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MyFit" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/blue_main</item>
    </style>
    <style name="Theme.MyFit" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/blue_main</item>
    </style>
    <style name ="TEXT">
        <item name="android:textColor">@color/dark_grey</item>
    </style>
    <style name="Theme">
        <item name="colorSurface">@color/blue_main</item>
        <item name="colorOnSurfaceVariant">@color/dark_grey</item>
    </style>
    <style name="HINT">
        <item name="android:hint">@color/grey</item>
    </style>
    <style name="Backgrounds">
        <item name="android:background">#E8E8E8</item>
    </style>
    <style name="Navigationcolor">
        <!--<item name="android:background">@drawable/backgroundnavigations3</item>-->
        <item name="android:background">#F7E6E6E6</item>
        <item name="android:theme">@style/Theme.MyFit</item>
        <item name="android:textColor">@style/TEXT</item>
        <item name="itemIconSize">@dimen/_25sdp</item>
    </style>

</resources>